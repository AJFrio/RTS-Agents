{
  "name": "rts-agents",
  "version": "1.0.0",
  "description": "RTS Agents Dashboard - Monitor Gemini CLI, Jules, and Cursor Cloud Agents",
  "scripts": {
    "build:css": "tailwindcss -i ./src/input.css -o ./styles/output.css --watch",
    "build:css:once": "tailwindcss -i ./src/input.css -o ./styles/output.css",
    "build:css:prod": "tailwindcss -i ./src/input.css -o ./styles/output.css --minify",
    "build:dist": "npm run build:css:prod && node scripts/build-dist.js",
    "dev:server": "node scripts/dev-server.js",
    "test": "jest",
    "test:e2e": "npm run build:css:prod && env -u DISPLAY xvfb-maybe playwright test",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug"
  },
  "keywords": [
    "rts",
    "agents",
    "gemini",
    "jules",
    "cursor",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-shell": "^2.3.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tauri-apps/cli": "^2.9.6",
    "@types/jest": "^29.5.14",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "playwright": "^1.57.0",
    "serve": "^14.2.0",
    "tailwindcss": "^3.4.0",
    "xvfb-maybe": "^0.2.1"
  }
}
