{
  "name": "rts-agents",
  "version": "1.0.0",
  "description": "RTS Agents Dashboard - Monitor Gemini CLI, Jules, and Cursor Cloud Agents",
  "main": "main.js",
  "scripts": {
    "start": "npm run build:css:prod && electron .",
    "dev": "concurrently \"npm run build:css\" \"electron .\"",
    "build:css": "tailwindcss -i ./src/input.css -o ./styles/output.css --watch",
    "build:css:prod": "tailwindcss -i ./src/input.css -o ./styles/output.css --minify",
    "test": "jest",
    "test:e2e": "npm run build:css:prod && env -u DISPLAY xvfb-maybe playwright test"
  },
  "keywords": [
    "electron",
    "rts",
    "agents",
    "gemini",
    "jules",
    "cursor",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "electron-store": "^8.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^29.5.14",
    "concurrently": "^9.2.1",
    "electron": "^28.0.0",
    "jest": "^29.7.0",
    "playwright": "^1.57.0",
    "tailwindcss": "^3.4.0",
    "xvfb-maybe": "^0.2.1"
  }
}
